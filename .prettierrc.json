{
	"semi": true,
	"tabWidth": 4,
	"useTabs": true,
	"printWidth": 120,
	"singleQuote": false,
	"trailingComma": "es5",
	"requirePragma": false,
	"bracketSpacing": true,
	"arrowParens": "always",
	"jsxSingleQuote": false,
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"jsxBracketSameLine": true,
	"embeddedLanguageFormatting": "auto"
}
